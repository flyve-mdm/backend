msgid "API"
msgstr ""
"INSERT INTO T_CATEGORY\n"
"SELECT\n"
"'base',\n"
"X.sub_name,\n"
"2,\n"
"cat_ord,\n"
"Y.idx,\n"
"0,\n"
"0,\n"
"sub_code ,\n"
"getdate()\n"
"FROM\n"
"(\n"
" SELECT\n"
" (SELECT MAX(idx) FROM T_CATEGORY) as newIdx,\n"
" LEFT(sub_code, 2) AS pCode,\n"
" sub_code,\n"
" sub_name,\n"
" Row_Number() Over(Partition By LEFT(sub_code, 2) ORDER BY sub_code ASC) AS cat_ord,\n"
" ROW_NUMBER() OVER(ORDER BY sub_code ASC) AS rowCnt\n"
" FROM\n"
" ERP_DB.LIFEBOOK.dbo.M_COMMON_CODE\n"
" WHERE 1=1\n"
" AND code_gubun = '003'\n"
" AND sub_code <> '01'\n"
") X\n"
"LEFT OUTER JOIN T_CATEGORY Y ON (X.pCode = Y.ERP_CATCD)"

