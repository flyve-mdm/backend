{% block tab_info %}
<tr class="tab_bg_1">
   <td>{{ __('Name') }}{{ withTemplate }}</td>
   <td><a href="{{ agentUrl }}?id={{ agent.id }}">{{ agent.name }}</a></td>
   <td>{{ __('Version') }}</td>
   <td>{{ agent.version }}</td>
</tr>
<tr class="tab_bg_1">
   <td>{{ __('Fleet', 'flyvemdm') }}</td>
   <td><a href="{{ fleetUrl }}?id={{ agent.plugin_flyvemdm_fleets_id }}">{{ agent.fleet.name }}</a>
   </td>
   <td>{{ __('Last contact', 'flyvemdm') }}</td>
   <td>{{ agent.last_contact }}</td>
</tr>
   <tr class="tab_bg_1">
      <td>{{ __('Notification type', 'flyvemdm') }}</td>
      <td>{{ agent.notification_type|upper }}</td>
      <td>{{ __('MDM Type', 'flyvemdm') }}</td>
      <td>{{ __(agent.mdm_type, 'flyvemdm') }}</td>
   </tr>
{% endblock %}